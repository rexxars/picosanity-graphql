{
  "name": "picosanity-graphql",
  "version": "1.0.2",
  "description": "Tiny GraphQL request library for Sanity",
  "main": "lib/index.js",
  "browser": "lib/browser.js",
  "umd": "umd/client.js",
  "unpkg": "umd/client.js",
  "scripts": {
    "clean": "rimraf lib/ umd/",
    "build": "npm run clean && npm run compile && NODE_ENV=production webpack -p",
    "compile": "babel --copy-files -d lib/ src/",
    "posttest": "eslint .",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rexxars/picosanity-graphql.git"
  },
  "keywords": [
    "sanity",
    "client",
    "graphql"
  ],
  "engines": {
    "node": ">=8"
  },
  "author": "Espen Hovlandsdal <espen@hovlandsdal.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rexxars/picosanity-graphql/issues"
  },
  "homepage": "https://github.com/rexxars/picosanity-graphql#readme",
  "dependencies": {
    "node-fetch": "^2.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-sanity": "^0.141.5",
    "graphql-query-compress": "^1.2.2",
    "jest": "^24.8.0",
    "prettier": "^1.13.5",
    "rimraf": "^2.6.2",
    "webpack": "^4.12.0",
    "webpack-command": "^0.4.2"
  }
}
